    "    k += 1\n",
    "    print(\"-----------------------------------------------------------------------\")\n",
    "    print(\"Training batch:\", k)\n",
    "    print(\"-----------------------------------------------------------------------\")\n",
    "    Y_pred = train(batch_input, y_star)\n",
    "    #print(Y_pred.size())\n",
    "    predList.append(Y_pred[:5])\n",
    "    if k == 5:\n",
    "        break\n",
    "batch_outputs = torch.stack(predList)\n",
